{
  "name": "hey-boss",
  "version": "2.0.0",
  "description": "Phone call input for Claude Code - Claude calls you when it needs your input",
  "author": "Hey Boss Contributors",
  "license": "MIT",
  "repository": "https://github.com/ZeframLou/hey-boss",
  "mcp-servers": [
    {
      "name": "hey-boss",
      "command": "npx",
      "args": ["-y", "hey-boss-mcp"],
      "transport": "stdio",
      "env": {
        "TWILIO_ACCOUNT_SID": "",
        "TWILIO_AUTH_TOKEN": "",
        "TWILIO_PHONE_NUMBER": "",
        "USER_PHONE_NUMBER": "",
        "OPENAI_API_KEY": "",
        "PUBLIC_URL": "",
        "PORT": "3000"
      }
    }
  ],
  "requirements": {
    "node": ">=18.0.0"
  }
}
